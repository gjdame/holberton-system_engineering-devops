#creates upstart config for 6-odd-or-even
description "Gunicorn application server running Airbnb_v2"

start on runlevel [2345]
stop on runlevel [!2345]

respawn
setuid ubuntu
setgid www-data


chdir /home/ubuntu/AirBnB_clone_v4
exec gunicorn --workers 3 --bind 0.0.0.0:8001 wsgi.wsgi_6:web_flask.app